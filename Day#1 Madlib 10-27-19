  	 <script>
  	 


const madLib = () => {
let name = prompt("Name main char");
let bar = prompt("Bar name");
let man = prompt("Another name");
let nasty = prompt("Something nasty");
let food = prompt("Name a food");
let car = prompt("What type of car")
let beer = prompt("Name a beer")
let msg = document.write(`<div class= "container" style="background-color: white; color: red; border-radius: 12px;"><div class="row"><div class="col-xs-12"><p><b>${name} decides to go to ${bar}. At ${bar} he orders some ${food} and a ${beer}.Then ${man} comes over and  throws ${nasty} in ${name}'s face. ${name} gets up and throws the glass of ${beer} at ${man}. The owner of ${bar} intervenes and ${man} rubs ${nasty} all over him. ${name} tackles ${man} to the ground, then the owner comes flying through the wall in his ${car} and knocks himself out and injured a few people including ${name}. <b><\p></div></div></div>`)



}
    
	
  	 </script>
